{
    /* 오퍼레이터 화면에서 가챠 성공여부 확인 */
    "Start_Check": {
        "focus": "▶ 가챠 확인중...",
        "next": "Check_Wait_Until_Operator"
    },
    "Check_Wait_Until_Operator": {
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "template": "check/1_operator.png",
                "roi": [
                    671,
                    0,
                    543,
                    149
                ], // "origin roi" : [721,15,443,49]
                "threshold": 0.95
            }
        },
        "next": "Check_Operator_Count"
    },
    "Check_Operator_Count": {
        "next": "Check_Operator_Name"
    },
    "Check_Operator_Count_Success": {
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "template": "check/2_6star.png",
                "roi": [
                    9,
                    77,
                    1270,
                    620
                ], // 화면 전체
                "threshold": 0.9,
                "green_mask": true
            }
        },
        "focus": "- 6성 1개 확인",
        "next": "Check_Operator_Name"
    },
    "Check_Operator_Name": {
        "next": "Check_Success"
    },
    "Check_Operator_Name_Success": {
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "template": "check/operator/exusiai_the_new_covenant.png",
                "roi": [
                    9,
                    77,
                    1270,
                    620
                ], // 화면 전체
                "threshold": 0.95
            }
        },
        "focus": "- 신시아 확인",
        "next": "Check_Success"
    },
    "Check_Success": {
        "focus": "!!! 리세 성공 !!!"
    },
    "Check_Failed": {
        "focus": "XXX 리세 실패 XXX",
        "next": "Start_Reset"
    }
}